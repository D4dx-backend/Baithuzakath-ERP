{
  "name": "baithuzkath-api",
  "version": "1.0.0",
  "description": "Baithuzzakath Kerala NGO Management System API",
  "main": "src/app.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "seed": "node scripts/seed.js",
    "seed:locations": "node scripts/seed.js locations",
    "seed:users": "node scripts/seed.js users",
    "seed:projects": "node scripts/seed.js projects",
    "seed:schemes": "node scripts/seed.js schemes",
    "seed:beneficiaries": "node scripts/seed.js beneficiaries",
    "seed:applications": "node scripts/seed.js applications",
    "seed:interviews": "node scripts/seed.js interviews",
    "seed:clear": "node scripts/seed.js clear",
    "seed:test": "node seed-applications.js",
    "clear:test": "node clear-test-data.js",
    "rbac:init": "node src/scripts/initRBACProduction.js",
    "rbac:debug": "node src/scripts/debugUserPermissions.js",
    "rbac:cleanup": "node -e \"require('./src/services/rbacService').cleanupExpiredAssignments().then(console.log)\"",
    "validate:nazim": "node src/scripts/validateNazimData.js",
    "import:nazim": "node src/scripts/importNazimData.js",
    "verify:nazim": "node src/scripts/verifyImport.js",
    "clear:nazim": "node src/scripts/clearNazimData.js"
  },
  "keywords": [
    "ngo",
    "zakat",
    "management",
    "api",
    "nodejs"
  ],
  "author": "Baithuzzakath Kerala",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.13.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "geoip-lite": "^1.4.10",
    "gridfs-stream": "^1.1.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.15.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
